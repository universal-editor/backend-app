{
    "name": "universal-editor/backend-app",
    "description": "Test API for the Universal editor",
    "keywords": ["universal-editor"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/universal-editor/backend-app/issues",
        "source": "https://github.com/universal-editor/backend-app"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "notamedia/yii2-relation": "master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
